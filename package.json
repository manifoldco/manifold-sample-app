{
  "name": "quickstart",
  "version": "0.0.1",
  "description": "Really quick guide to use lodna with Manifold",
  "main": "server.js",
  "scripts": {
    "client": "babel src/client -d dist/client --ignore 'src/**/*.test.js'",
    "client:watch": "babel src/client --watch -d dist/client --ignore 'src/**/*.test.js'",
    "dev": "manifold run 'yarn dev:command'",
    "dev:command": "concurrently 'npm run server:watch' 'npm run client:watch' 'npm run postcss:watch' 'nodemon dist/server'",
    "build": "npm run client && npm run server && npm run postcss",
    "postcss": "postcss src/client/main.css -o dist/client/assets/main.css",
    "postcss:watch": "postcss src/client/main.css -o dist/client/assets/main.css -w",
    "seed": "node dist/server/tasks/seed",
    "server": "babel src/server -d dist/server --ignore 'src/**/*.test.js'",
    "server:watch": "babel src/server --watch -d dist/server --ignore 'src/**/*.test.js'",
    "start": "nodemon dist/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "bcrypt": "^1.0.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "dotenv-expand": "^4.2.0",
    "express": "^4.16.3",
    "mailgun-js": "^0.16.0",
    "nodemon": "^1.15.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "postcss": "^6.0.21",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "sequelize": "^4.37.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
